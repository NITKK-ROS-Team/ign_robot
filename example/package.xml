<?xml version="1.0"?>
<package format="2">
  <name>ign_robot</name>
  <version>0.4.1</version>
  <description>ign_ros2_control_demos</description>

  <maintainer email="ahcorde@osrfoundation.org">Alejandro Hernandez</maintainer>

